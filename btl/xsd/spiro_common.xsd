            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="date" type="BtlDateTime" />
        <xs:attribute name="status">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="best"/>
